def assemble_genome(dna_list): #defines function
    """
    :type dna_list: List[str] 
    :rtype: str 
    """

    if len(dna_list)==1: #defined string up above
        return dna_list[0]
    dic={} #establishes dictionary
    for i in range(len(dna_list)): #relates the given list to the string code
        for j in range(len(dna_list)):
            if i!=j:
                d=0
                for k in range(1,min(len(dna_list[i]),len(dna_list[j]))):
                    if dna_list[j][:k]==dna_list[i][-k:]:
                        d=k
                dic[(i,j)]=d
    if max(dic.values())==0:
        return "".join(dna_list) #prevents error for if equal to zero
    else:
        string="".join(dna_list) #joins the string that string that is given
        l=len(string)
        stack=[]
        for i,w in enumerate(dna_list): #keeps track of how many iterations have occured
            t=set(range(len(dna_list)))
            t.remove(i)
            stack.append((w,i,t)) #adds dna_list to the string
        while stack:
            a,c,remain=stack.pop()
            if len(a)<l:
                if not remain:
                    string=a
                    l=len(string)
                else:
                    t=[[dic[c,x],x] for x in remain] #overlaps common points of string
                    t.sort()
                    for d,x in t:
                        na=a+dna_list[x][d:]
                        nremain=set(remain)
                        nremain.remove(x)
                        stack.append((na,x,nremain)) #removes excess loops and leaves remaining superstring
        return string
